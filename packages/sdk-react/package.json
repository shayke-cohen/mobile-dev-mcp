{
  "name": "@mobile-dev-mcp/react",
  "version": "0.1.0",
  "description": "React SDK for Mobile Dev MCP - AI-assisted web development with Cursor IDE",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean --tsconfig tsconfig.build.json",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch --tsconfig tsconfig.build.json",
    "lint": "eslint -c ../../eslint.config.js src/",
    "typecheck": "tsc --noEmit -p tsconfig.build.json",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "peerDependencies": {
    "react": ">=17.0.0",
    "react-dom": ">=17.0.0"
  },
  "devDependencies": {
    "@testing-library/react": "^14.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "jsdom": "^22.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "keywords": [
    "react",
    "mcp",
    "cursor",
    "ai",
    "debugging",
    "development",
    "devtools"
  ],
  "author": "Mobile Dev MCP",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/anthropics/mobile-dev-mcp.git",
    "directory": "packages/sdk-react"
  },
  "bugs": {
    "url": "https://github.com/anthropics/mobile-dev-mcp/issues"
  },
  "homepage": "https://github.com/anthropics/mobile-dev-mcp#readme"
}
